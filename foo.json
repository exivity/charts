{
  "db": {
    "driver": "postgres",
    "parameters": {
      "connect_timeout": 10,
      "dbname": "exivity",
      "host": "db",
      "password": "Password12!",
      "port": 5432,
      "sslmode": "disable",
      "user": "postgres"
    }
  },
  "log": {
    "driver": "file",
    "parameters": {
      "filename": "{component}.log"
    }
  },
  "merlin": {
    "heartbeatPeriod": 5,
    "programs": {
      "extract": {
        "CPU": 0.25,
        "RAM": 100,
        "component": "use",
        "params": "${params}",
        "path": "bin/use",
        "queue": "EXTRACT"
      }
    },
    "reservedCPU": 1
  },
  "mq": {
    "password": "guest",
    "redialPeriod": 5,
    "servers": [
      {
        "host": "rabbit",
        "port": 5672,
        "secure": false
      }
    ],
    "user": "guest",
    "vhost": "/"
  }
}
